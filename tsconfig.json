{
    "compilerOptions": {
      "target": "esnext",
      "lib": ["dom", "esnext"],
      "jsx": "react-jsx",
      "strict": true,
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  